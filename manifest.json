{   
  "name": "YouTube Image Previews",
  "description": "Get any image thumbnails associated with a particular YouTube video.",
  "permissions": [ "tabs", "http://www.youtube.com/*" ],
  "version": "0.2",
  "manifest_version":2,
  "icons": { 
    "16": "img/youtube-image-previews-icon-16.png",
    "48": "img/youtube-image-previews-icon-48.png",
    "128": "img/youtube-image-previews-icon-128.png"
  },
  "page_action": {
    "default_icon": "img/youtube-image-previews-icon-19.png",
    "name": "Click to maximize in the tab",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}
